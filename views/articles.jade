extends layout
block content
  div.wrapper-articles
    section.article
      h1 #{title}
      ul
        - each article in articles
          li
            a(href="/blog/admin/articles/#{article.slug}") #{article.title}
      p.action
        a.link.new(href='/blog/admin/users/new') Nuevo
        a.link(href='/blog/admin') Volver